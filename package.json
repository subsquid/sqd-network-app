{
  "name": "@subsquid/subsquid-web",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "exports": "./src/index.tsx",
  "scripts": {
    "analyze": "npx vite-bundle-visualizer",
    "build": "vite build --outDir build",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\" --fix",
    "tsc": "tsc --noEmit",
    "start": "vite serve --port 3005 --host 127.0.0.1",
    "preview": "vite preview --outDir build --host 127.0.0.1 --port 3005",
    "upg": "yarn upgrade-interactive",
    "prepare": "husky install",
    "codegen:graphql": "npx graphql-codegen --config graphql.config.ts",
    "codegen:wagmi": "wagmi generate",
    "codegen": "npm run codegen:graphql && npm run codegen:wagmi"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  },
